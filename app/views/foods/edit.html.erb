<%= form_for @food do |f| %>
	
	<div class="form-group">
		<%= f.text_field :name, placeholder: 'Food Name', class: 'form-control' %>
		<br>
		<%= f.text_field :price, placeholder: 'Price', class: 'form-control' %>
		<br>
		<%= f.submit "Submit", class: 'btn btn-primary' %>
		<%=link_to "Cancel", foods_path %>
	</div>


<% end %>

